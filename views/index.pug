extends layout

block content
  div.container-fluid
    div.content
      div.page-header
        h1 SDN Event
          small December 2016
      div.row
        p.col-md-12 Schedule
      div.clearfix
        for session in sessions 
          div.row.bg-primary
            div.col-md-8 
              h3= session.title
            div.col-md-4= session.starttime
          div.row
            div.col-md-12= session.speaker       
          div.clearfix

  footer.container-fluid
    div.row
      div.col-md-4 #{env}
      div.col-md-8 #{insights}